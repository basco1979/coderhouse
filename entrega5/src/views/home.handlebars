<div>
    <h2>Listado de Productos</h2>
    <table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Price</th>
    <th>Code</th>
    <th>Stock</th>
    <th>Status</th>
    <th>Category</th>
  </tr>
    {{#each listado}}
  <tr>
    <td>{{this.title}}</td>
    <td>{{this.description}}</td>
    <td>{{this.price}}</td>
    <td>{{this.code}}</td>
    <td>{{this.stock}}</td>
    <td>{{this.status}}</td>
    <td>{{this.category}}</td>
  </tr>
    {{/each}}
</table>
</div>